<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Company Visits -->
        <record id="seq_company_visit" model="ir.sequence">
            <field name="name">Company Visit Sequence</field>
            <field name="code">company.visit</field>
            <field name="prefix">VISIT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>